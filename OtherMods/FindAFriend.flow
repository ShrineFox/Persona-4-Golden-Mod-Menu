void check_if_holiday()
{
    // This is the logic lmap uses so it should be right
    if (DATE_CHK(4, 12) == 1 || GET_TIME_OF_DAY() == 3)
    {
        isHoliday = 1;
    }
    else
    {
        isHoliday = 0;
    }
