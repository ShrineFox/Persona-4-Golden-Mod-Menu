    if (BIT_CHK(0 + 0x0400 + 475) == 0 || GET_SL_LEVEL(25) >= 10 || GET_SL_LEVEL(26) >= 10 || (GET_SL_LEVEL(25) > 0 && GET_SL_LEVEL(26) <= 0) || timeOfDay == 5)
